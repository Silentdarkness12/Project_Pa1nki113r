ACTOR EmptyBrassMP40
{
   Height 2
   Radius 2
   Speed 8
   Scale 0.15
   +DOOMBOUNCE
   - NOGRAVITY
   +WINDTHRUST
   +CLIENTSIDEONLY
   +MOVEWITHSECTOR
   +MISSILE
   +NOBLOCKMAP
   -DROPOFF
   +NOTELEPORT
   +FORCEXYBILLBOARD
   +NOTDMATCH
   +GHOST
   BounceCount 4
   Mass 1
   SeeSound "weapons/smcasebounce"
   States
   {
   Spawn:
      MPB3 ABCDEFGH 3 A_JumpIf(waterlevel > 1, "Splash")
      Loop

   Death:
      TNT1 A 0 A_StopSoundEx("weapons/smcasebounce")
	  TNT1 A 0 A_PlaySound("decasebounce")
      LCPJ A 0 A_Jump(256,"Rest1","Rest2","Rest3","Rest4","Rest5","Rest6","Rest7","Rest8")
      Goto Rest1
    Rest1:
      MPB3 I 250
      TNT1 A 0 A_CheckSight("Vanish")
    Loop
    Rest2:
      MPB3 J 250
      TNT1 A 0 A_CheckSight("Vanish")
    Loop
    Rest3:
      MPB3 K 250
      TNT1 A 0 A_CheckSight("Vanish")
    Loop
    Rest4:
      MPB3 L 250
      TNT1 A 0 A_CheckSight("Vanish")
    Loop
    Rest5:
      MPB3 M 250
      TNT1 A 0 A_CheckSight("Vanish")
    Loop
    Rest6:
      MPB3 I 250
      TNT1 A 0 A_CheckSight("Vanish")
    Loop
    Rest7:
      MPB3 J 250
      TNT1 A 0 A_CheckSight("Vanish")
    Loop
    Rest8:
      TNT1 A 0 A_CheckSight("Vanish")
      MPB3 K 250
    Loop
     Vanish:
     TNT1 A 0
     Stop
	 
	Splash:
	TNT1 A 0
	TNT1 A 0 A_SpawnItem("UnderwaterEmptyBrass")
	Stop
   }
}

