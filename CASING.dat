ACTOR underwaterEmptyBrass: EmptyBrass
{
   Speed 0
   -DOOMBOUNCE
   Mass 1
   SeeSound "none"
   States
   {
   Spawn:
      BLUD C 0 ThrustThingZ (0,7,1,0)
      CAS3 ABCD 3 
	  BLUD C 0 ThrustThingZ (0,7,1,0)
	  CAS3 EFGH 3
	  BLUD C 0 ThrustThingZ (0,7,1,0)
      CAS3 ABCD 3 
	  BLUD C 0 ThrustThingZ (0,7,1,0)
	  CAS3 EFGH 3
	   TNT1 A 0 A_SpawnItem("BUBULZ")
      Loop
   }
}

